    <integer-array name="swipe_refresh">
        <item>@color/deep_sky_blue</item>
        <item>@color/teal</item>
        <item>@color/bright_light_blue</item>
        <item>@color/sun_yellow</item>
        <item>@color/bright_orange</item>
