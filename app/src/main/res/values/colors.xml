<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="color_primary">@color/white</color>
    <color name="color_primary_dark">@color/white</color>
    <color name="color_accent">@color/black</color>
    <color name="color_on_secondary">@color/black</color>
    <color name="color_surface">@color/white</color>
    <color name="color_control_light">#39000000</color> <!-- Same as textColorPrimary disabled -->

    <!-- Scrim colors -->
    <color name="system_ui_scrim_light">#B3FFFFFF</color> <!-- 70% white -->
    <color name="system_ui_scrim_black">#40000000</color> <!-- 25% black -->

    <!-- We use a dark scrim by default since light status is API 23+ -->
    <color name="status_bar_scrim">@color/system_ui_scrim_black</color>
    <!-- We use a dark scrim by default since light nav bar is API 27+ -->
    <color name="nav_bar_scrim">@color/system_ui_scrim_black</color>

    <color name="white">#FFFFFF</color>
    <color name="black">#000000</color>
    <color name="gray">#CCCCCC</color>
    <color name="littleYellow">#FFCE84</color>
    <color name="littlePink">#FDA3A9</color>
    <color name="suchPink">#FB76A3</color>
    <color name="mindPink">#F972CC</color>
    <color name="littlePurple">#CB70EE</color>
    <color name="suchPurple">#9886F2</color>

    <color name="deep_sky_blue">#1a73e8</color> <!-- colorPrimary in Light mode-->
    <color name="warm_blue">#574ddd</color> <!-- colorPrimaryDark in Light mode-->
    <color name="carolina_blue">#8ab4f8</color> <!-- colorPrimary in Dark mode-->
    <color name="cornflower_blue">#669DF6</color> <!-- colorPrimaryDark in Dark mode-->

    <color name="teal">#069f86</color>
    <color name="bright_light_blue">#27e5fd</color>
    <color name="sun_yellow">#fcd230</color>
    <color name="bright_orange">#ff6c00</color>
    <color name="transparent">#0fff</color>

    <integer-array name="swipe_refresh">
        <item>@color/littleYellow</item>
        <item>@color/littlePink</item>
        <item>@color/suchPink</item>
        <item>@color/mindPink</item>
        <item>@color/littlePurple</item>
        <item>@color/suchPurple</item>
    </integer-array>
</resources>
