<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorPrimary">@color/white</color>
    <color name="colorPrimaryDark">@color/white</color>
    <color name="colorAccent">@color/black</color>

    <color name="white">#FFFFFF</color>
    <color name="black">#000000</color>
    <color name="gray">#CCCCCC</color>
    <color name="littleYellow">#FFCE84</color>
    <color name="littlePink">#FDA3A9</color>
    <color name="suchPink">#FB76A3</color>
    <color name="mindPink">#F972CC</color>
    <color name="littlePurple">#CB70EE</color>
    <color name="suchPurple">#9886F2</color>

    <color name="deep_sky_blue">#1a73e8</color> <!-- colorPrimary in Light mode-->
    <color name="warm_blue">#574ddd</color> <!-- colorPrimaryDark in Light mode-->
    <color name="carolina_blue">#8ab4f8</color> <!-- colorPrimary in Dark mode-->
    <color name="cornflower_blue">#669DF6</color> <!-- colorPrimaryDark in Dark mode-->

    <color name="teal">#069f86</color>
    <color name="bright_light_blue">#27e5fd</color>
    <color name="sun_yellow">#fcd230</color>
